# AWS S3 Configuration (VITE_ prefix required for frontend access)
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
VITE_AWS_REGION=us-east-1
VITE_AWS_S3_BUCKET=your_s3_bucket_name

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Other environment variables
NODE_ENV=development
NETLIFY_API_TOKEN=your_netlify_token_here